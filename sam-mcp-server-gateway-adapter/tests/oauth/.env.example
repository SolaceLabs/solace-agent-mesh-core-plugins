# OAuth Test Client Configuration
# Copy this file to .env and customize as needed

# Server URLs
MCP_SERVER_URL=http://localhost:8090
AUTH_PROXY_URL=http://localhost:8080

# Callback server
CALLBACK_HOST=127.0.0.1
CALLBACK_PORT=8888

# Timeouts (seconds)
AUTHORIZATION_TIMEOUT=300

# Options
VERBOSE=true
AUTO_OPEN_BROWSER=true
RESULTS_DIR=./test_results

# Optional: Pre-registered client credentials
# Leave empty to use dynamic registration
DEFAULT_CLIENT_ID=
DEFAULT_CLIENT_SECRET=
